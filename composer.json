{
    "name": "alamindev27/laravel-api-responder",
    "description": "A simple and consistent API response helper for Laravel.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alamindev27\\ApiResponder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "MD Al-Amin",
            "email": "alamindev27@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Alamindev27\\ApiResponder\\ApiResponderServiceProvider"
            ],
            "aliases": {
                "ApiResponder": "Alamindev27\\ApiResponder\\Facades\\ApiResponder"
            }
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^7.2|^8.0|^9.0|^10.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.9"
    }
}
